-- Fetching UI Library - Complete Usage Example
-- T·∫•t c·∫£ element v√† c√°ch s·ª≠ d·ª•ng

-- Load th∆∞ vi·ªán (thay URL c·ªßa b·∫°n)
local FetchingUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/yeuemne/Uimake/refs/heads/main/SoucreUiMains12.lua"))()

-- T·∫°o Window ch√≠nh
local window = Fetching:Window({
    Title = "Complete UI Test",
    Logo = "4483362458", -- ID h√¨nh ·∫£nh
    Size = UDim2.new(0, 490, 0, 380),
    MainColor = Color3.fromRGB(255, 64, 129),
    DropColor = Color3.fromRGB(156, 39, 176),
    Keybind = Enum.KeyCode.RightControl -- Ph√≠m m·ªü/ƒë√≥ng
})

print("Window created!")

-- ========================================
-- TAB 1: BASIC ELEMENTS
-- ========================================
local basicTab = Fetching.Tab:CreateTab({
    Title = "Basic Elements",
    Icon = "6031075938"
})

-- Section tr√°i
local basicLeftSection = Fetching.Section:CreateSection({
    Title = "Text Elements",
    Side = "left"
})


local label1 = Fetching.Main:CreateLabel({
    Title = "This is a centered label",
    Side = "center"
})

local label2 = Fetching.Main:CreateLabel({
    Title = "Left aligned label",
    Side = "left"
})

local label3 = Fetching.Main:CreateLabel({
    Title = "Right aligned label", 
    Side = "right"
})

-- 2. Create (Text Card)
local textCard = Fetching.Main:Create({
    Title = "Information Card",
    Desc = "This is a description text that can be quite long and will wrap to multiple lines if needed.",
    Icon = "6031068421"
})

-- 3. Paragraph (RichText)
local paragraph = Fetching.Main:Paragarp({
    Title = "Welcome to <b>Fetching UI</b>! This library supports <i>rich text</i> formatting."
})

-- 4. Line separator
local line1 = Fetching.Main:Line()

-- 5. Icon display
local icon1 = Fetching.Main:Icon({
    Title = "Premium Feature",
    Image = "6031068421",
    Color = Color3.fromRGB(255, 215, 0)
})

-- 6. Status indicator
local status1 = Fetching.Main:CreateSpawn({
    Title = "Server Status: Online",
    Color = Color3.fromRGB(76, 175, 80)
})

-- Section ph·∫£i
local basicRightSection = Fetching.Section:CreateSection({
    Title = "Interactive Elements",
    Side = "right"
})

-- 7. Button
local button1 = Fetching.Main:CreateButton({
    Title = "Click Me!",
    Callback = function()
        print("Button clicked!")
        label1:Set("Button was clicked at " .. os.date("%X"))
    end
})

-- 8. TextBox
local textbox1 = Fetching.Main:CreateTextbox({
    Title = "Enter Text",
    Placeholder = "Type something...",
    Value = "Default text",
    ClearText = false,
    Callback = function(text)
        print("Text entered:", text)
        paragraph:Set("You typed: <b>" .. text .. "</b>")
    end
})

-- 9. Toggle
local toggle1 = Fetching.Main:CreateToggle({
    Title = "Enable Feature",
    Value = false,
    CallBack = function(state)
        print("Toggle state:", state)
        status1:SetColor(state and Color3.fromRGB(76, 175, 80) or Color3.fromRGB(244, 67, 54))
        status1:SetTitle("Feature: " .. (state and "ENABLED" or "DISABLED"))
    end
})

-- 10. ImageToggle
local imageToggle1 = Fetching.Main:ImageToggle({
    Title = "Visual Toggle",
    Value = false,
    Icon = "6031068421",
    CallBack = function(state)
        print("Image toggle:", state)
        icon1:ImageColor(state and Color3.fromRGB(76, 175, 80) or Color3.fromRGB(244, 67, 54))
    end
})

-- ========================================
-- TAB 2: ADVANCED ELEMENTS  
-- ========================================
local advancedTab = Fetching.Tab:CreateTab({
    Title = "Advanced",
    Icon = "6031068421"
})

-- Section tr√°i - Sliders v√† Selects
local advancedLeftSection = Fetching.Section:CreateSection({
    Title = "Value Controls",
    Side = "left"
})

-- 11. Slider
local slider1 = Fetching.Main:CreateSlider({
    Title = "Speed Value",
    Desc = "Adjust speed", 
    Min = 0,
    Max = 100,
    Rounding = 1,
    Value = 50,
    CallBack = function(value)
        print("Slider value:", value)
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
        end
    end
})

-- 12. Select (v·ªõi m≈©i t√™n)
local select1 = Fetching.Main:CreateSelect({
    Title = "Game Mode",
    Desc = "Choose mode",
    Value = "Normal",
    List = {"Easy", "Normal", "Hard", "Expert", "Nightmare"},
    Callback = function(selected)
        print("Selected mode:", selected)
        textCard:SetTitle("Mode: " .. selected)
        textCard:SetDesc("You have selected " .. selected .. " difficulty mode.")
    end
})

-- 13. Keybind
local keybind1 = Fetching.Main:CreateKeybind({
    Title = "Toggle Hotkey",
    Key = Enum.KeyCode.Q,
    Value = false,
    Callback = function(key, state)
        print("Keybind:", key, "State:", state)
        toggle1:SetValue(state)
    end
})

-- 14. Single Dropdown
local dropdown1 = Fetching.Main:CreateDropdown({
    Title = "Select Weapon",
    List = {"Sword", "Bow", "Staff", "Hammer", "Dagger"},
    Value = "Sword",
    Multi = false,
    Callback = function(selected)
        print("Selected weapon:", selected)
    end
})

-- 15. Multi Dropdown  
local dropdown2 = Fetching.Main:CreateDropdown({
    Title = "Enable Features",
    List = {"Auto-Farm", "Anti-AFK", "Speed Boost", "Jump Boost", "ESP"},
    Value = {"Auto-Farm"},
    Multi = true,
    Callback = function(selected)
        print("Enabled features:", table.concat(selected, ", "))
    end
})

-- Section ph·∫£i - Media Elements
local advancedRightSection = Fetching.Section:CreateSection({
    Title = "Media Elements",
    Side = "right"
})

-- 16. Image display
local image1 = Fetching.Main:Image({
    Title = "Logo Image",
    Image = "4483362458"
})


-- 18. CreateImage (Info card with image)
local imageCard = Fetching.Main:CreateImage({
    Title = "Server Information", 
    Desc
